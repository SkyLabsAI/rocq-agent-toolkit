{
  "id": null,
  "title": "üìä Custom Eval Pipeline",
  "description": "Generic monitoring dashboard for pipelines with success/failure rates and KC distribution analysis",
  "tags": ["psi-verifier", "pipeline", "monitoring", "observability"],
  "style": "dark",
  "timezone": "browser",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "templating": {
    "list": [
      {
        "name": "time_range",
        "type": "interval",
        "label": "Time Range",
        "auto": true,
        "auto_count": 30,
        "auto_min": "10s",
        "current": {
          "text": "auto",
          "value": "$__auto_interval_time_range"
        },
        "hide": 0,
        "includeAll": false,
        "multi": false,
        "options": [
          {"text": "auto", "value": "$__auto_interval_time_range", "selected": true},
          {"text": "1m", "value": "1m", "selected": false},
          {"text": "5m", "value": "5m", "selected": false},
          {"text": "10m", "value": "10m", "selected": false},
          {"text": "30m", "value": "30m", "selected": false},
          {"text": "1h", "value": "1h", "selected": false}
        ]
      },
      {
        "current": {
          "selected": false,
          "text": "eval_dataset_inference",
          "value": "eval_dataset_inference"
        },
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "definition": "label_values(service_name)",
        "hide": 0,
        "includeAll": false,
        "label": "Service Name",
        "multi": false,
        "name": "service_name",
        "options": [],
        "query": "label_values(service_name)",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      },
      {
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "datasource": {
          "type": "loki",
          "uid": "Loki"
        },
        "definition": "sum by(run_id) (count_over_time({service_name=\"$service_name\"} | json | run_id!=\"\" [$__range]))",
        "hide": 0,
        "includeAll": true,
        "label": "Run ID",
        "multi": false,
        "name": "run_id",
        "options": [],
        "query": "sum by(run_id) (count_over_time({service_name=\"$service_name\"} | json | run_id!=\"\" [$__range]))",
        "refresh": 2,
        "regex": "run_id=\\\"([^\\\"]+)\\\"",
        "skipUrlSync": false,
        "sort": 0,
        "type": "query"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "title": "üìà PIPELINE PERFORMANCE OVERVIEW",
      "type": "row",
      "collapsed": false,
      "datasource": null,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "‚úÖ Success Rate",
      "type": "gauge",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "sum(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=\"success\" [$__range])) / sum(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=~\"success|error\" [$__range])) * 100",
          "legendFormat": "Success Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "green",
                "value": 90
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "9.0.0",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      }
    },
    {
      "id": 3,
      "title": "‚ùå Failure Rate",
      "type": "gauge",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "sum(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=\"error\" [$__range])) / sum(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=~\"success|error\" [$__range])) * 100",
          "legendFormat": "Failure Rate %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10
              },
              {
                "color": "red",
                "value": 30
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "9.0.0",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      }
    },
    {
      "id": 4,
      "title": "üîÑ KC GENERATION ANALYSIS",
      "type": "row",
      "collapsed": false,
      "datasource": null,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 9
      }
    },
    {
      "id": 5,
      "title": "üìä KC Distribution (generation_kc)",
      "type": "piechart",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "sum by (generation_kc)(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=\"success\" | generation_kc!=\"\" [$__range]))",
          "legendFormat": "KC {{generation_kc}}",
          "refId": "A"
        },
        {
          "expr": "sum(count_over_time({service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=\"error\" [$__range]))",
          "legendFormat": "Error",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            }
          },
          "mappings": [],
          "unit": "short"
        }
      },
      "options": {
        "pieType": "pie",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "values": ["percent"]
        },
        "displayLabels": ["name", "percent"]
      },
      "pluginVersion": "9.0.0",
      "gridPos": {
        "h": 10,
        "w": 18,
        "x": 0,
        "y": 10
      }
    },
    {
      "id": 9,
      "title": "File Processing Summary",
      "type": "stat",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "sum(last_over_time({service_name=\"$service_name\"} | json | run_id=~\"$run_id\" | line_format \"{{.message}}\" | regexp \"Processed (?P<files>\\\\d+) files, (?P<successful>\\\\d+) successful, (?P<failed>\\\\d+) failed\" | unwrap files [$__range]))",
          "legendFormat": "Processed",
          "refId": "A"
        },
        {
          "expr": "sum(last_over_time({service_name=\"$service_name\"} | json | run_id=~\"$run_id\" | line_format \"{{.message}}\" | regexp \"Processed (?P<files>\\\\d+) files, (?P<successful>\\\\d+) successful, (?P<failed>\\\\d+) failed\" | unwrap successful [$__range]))",
          "legendFormat": "Successful",
          "refId": "B"
        },
        {
          "expr": "sum(last_over_time({service_name=\"$service_name\"} | json | run_id=~\"$run_id\" | line_format \"{{.message}}\" | regexp \"Processed (?P<files>\\\\d+) files, (?P<successful>\\\\d+) successful, (?P<failed>\\\\d+) failed\" | unwrap failed [$__range]))",
          "legendFormat": "Failed",
          "refId": "C"
        }
      ],
      "gridPos": {
        "h": 10,
        "w": 6,
        "x": 18,
        "y": 10
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ],
          "fields": ""
        },
        "orientation": "vertical",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      }
    },
    {
      "id": 6,
      "title": "üìù DETAILED LOGS & DEBUGGING",
      "type": "row",
      "collapsed": false,
      "datasource": null,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 20
      }
    },
    {
      "id": 7,
      "title": "üîç Recent Logs",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "{service_name=\"$service_name\", run_id=~\"$run_id\"} | json",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": true,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      },
      "pluginVersion": "9.0.0",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 21
      }
    },
    {
      "id": 8,
      "title": "üö® Error Logs Only",
      "type": "logs",
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "expr": "{service_name=\"$service_name\", run_id=~\"$run_id\"} | json | brick_server_result_status=\"error\"",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": true,
        "wrapLogMessage": false,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      },
      "pluginVersion": "9.0.0",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 33
      }
    }
  ],
  "time": {
    "from": "now-2h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  },
  "refresh": "30s",
  "schemaVersion": 35,
  "version": 1
} 